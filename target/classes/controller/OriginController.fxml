<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="650.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.OriginController">
   <children>
      <ImageView fitHeight="650.0" fitWidth="853.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../background.jpg" />
         </image>
      </ImageView>
      <Label alignment="TOP_CENTER" layoutX="189.0" layoutY="8.0" prefHeight="118.0" prefWidth="401.0" text="2048" textAlignment="CENTER" textFill="#473f8a">
         <font>
            <Font size="92.0" />
         </font>
      </Label>
      <Button fx:id="play" layoutX="283.0" layoutY="309.0" mnemonicParsing="false" onAction="#Play" text="Tourist visit">
         <font>
            <Font size="31.0" />
         </font>
      </Button>
      <Button fx:id="Login" layoutX="283.0" layoutY="203.0" mnemonicParsing="false" onAction="#Login_Event" prefHeight="62.0" prefWidth="213.0" text="Log in">
         <font>
            <Font size="31.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
